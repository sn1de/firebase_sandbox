<h1>Add New Counter</h1>
<input type="text" id="counterName" placeholder="Name">
<script type="text/javascript">
	$('#counterName').keypress(function(e) {
		if (e.keyCode == 13) {
			var name = $('#counterName').val();
			data.push({counter: name, count: 0});
			$('#counterName').val('');
		}
	});
</script>